    android:orientation="vertical"
    tools:context=".Fragments.InicioFragment">

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/contenedor"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/forma_imagen"
        app:layout_constraintBottom_toBottomOf="@+id/Contenedor2"
        app:layout_constraintLeft_toLeftOf="@+id/Contenedor3"
        app:layout_constraintRight_toRightOf="@+id/Contenedor4"
        app:layout_constraintTop_toTopOf="@+id/Contenedor1" />

    <me.biubiubiu.justifytext.library.JustifyTextView
        android:id="@+id/texto"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/infoInicio"
        android:textAlignment="center"
        android:textColor="@color/blanco"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/text2"
        app:layout_constraintLeft_toLeftOf="@id/text3"
        app:layout_constraintRight_toRightOf="@id/text4"
        app:layout_constraintTop_toTopOf="@+id/text1" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Inicio"
