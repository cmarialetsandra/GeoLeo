        android:textColor="@color/blanco"
        android:textSize="40dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/glAbajoTitulo"
        app:layout_constraintLeft_toLeftOf="@id/text3"
        app:layout_constraintRight_toRightOf="@id/text4"
        app:layout_constraintTop_toTopOf="@+id/glArribaTitulo" />

    <!--Guías líneas del Contenedor-->
    <androidx.constraintlayout.widget.Guideline
